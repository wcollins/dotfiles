{
  "permissions": {
    "allow": [
      "WebSearch",
      "WebFetch(domain:github.com)",
      "WebFetch(domain:draculatheme.com)",
      "WebFetch(domain:raw.githubusercontent.com)",
      "Bash(gh api:*)",
      "Bash(grep:*)",
      "Bash(head:*)",
      "Bash(echo starship not found in PATH:*)",
      "Bash(STARSHIP_CONFIG=~/.config/starship.toml starship prompt:*)",
      "Bash(echo TMUX=$TMUX:*)",
      "Bash(git clone:*)",
      "Bash(tmux show-option:*)",
      "Bash(tmux list-sessions:*)",
      "Bash(tmux display-message:*)"
    ]
  }
}
